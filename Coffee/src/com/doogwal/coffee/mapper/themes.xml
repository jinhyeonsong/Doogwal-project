<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC	
"-//mybatis.org//DTD Mapper 3.0//EN"	
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="themes">
<!-- 송진현 -->
<select id="selectOne" parameterType="int" resultType="Theme">
		SELECT theme
		FROM themes
		WHERE no=#{no}
	</select>
	<!-- end 송진현 -->
</mapper>  
	
